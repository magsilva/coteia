<Directory "$PATH_UPLOAD">
  Options -ExecCGI
  AllowOverride None
  RemoveHandler .jsp .php
</Directory>

<Directory "$PATH_COWEB/tables">
	<Limit GET POST OPTIONS PROPFIND>
		Order deny,allow
		Deny from all
	</Limit>
  Options -ExecCGI
  AllowOverride None
  RemoveHandler .jsp .php .html .htm
</Directory>
